{
	"name": "p2made/p2y2-rbac-users",
	"description": "RBAC-first user management for Yii2 (p2y2 ecosystem); a foundation for replacing 2amigos/yii2-usuario over time.",
	"type": "yii2-extension",
	"keywords": [
		"yii2", "yii2-extension", "rbac", "user-management",
		"authentication", "authorization", "acl", "roles", "permissions", "admin",
		"p2y2", "p2made"
	],
	"homepage": "https://github.com/p2made/p2y2-rbac-users",
	"license": "MIT",
	"authors": [
		{
			"name": "Pedro Plowman",
			"email": "pedrofp@me.com",
			"homepage": "https://github.com/p2made"
		}
	],
	"support": {
		"issues": "https://github.com/p2made/p2y2-rbac-users/issues?state=open",
		"source": "https://github.com/p2made/p2y2-rbac-users"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"p2made/p2y2-things": "dev-5.0-dev",
		"2amigos/yii2-usuario": "dev-v2.0.0-dev"
	},
	"autoload": {
		"psr-4": {
			"p2m\\rbac\\": ""
		}
	},
	"extra": {
		"yii\\extension": {
			"bootstrap": "p2m\\rbac\\i18n\\Bootstrap"
		}
	}}
